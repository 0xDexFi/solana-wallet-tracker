# Telegram Bot Token from @BotFather
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Helius API Key from helius.dev
HELIUS_API_KEY=your_helius_api_key_here

# Public URL where webhook server is accessible (e.g., ngrok URL for local dev)
WEBHOOK_URL=https://your-domain.com/webhook

# Database path (optional, defaults to data/wallets.db)
DATABASE_PATH=data/wallets.db

# Chat ID to send alerts to (your Telegram user ID or group ID)
TELEGRAM_CHAT_ID=your_chat_id_here
